{
  "expo": {
    "name": "FixKar",
    "slug": "fixkar",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "scheme": "servicemarketplace",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#00ACC1"
    },
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.fixkar.app",
      "config": {
        "googleMapsApiKey": "AIzaSyBhYMnd2kw3IravpOpUScd3XC0LXUS2hlY"
      }
    },
    "android": {
      "package": "com.fixkar.app",
      "permissions": [
        "ACCESS_FINE_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyBhYMnd2kw3IravpOpUScd3XC0LXUS2hlY"
        }
      },
      "versionCode": 1,
      "ndk": {
        "abiFilters": [
          "arm64-v8a",
          "arm64-v8a"
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FixKar to access your camera to upload photos"
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow FixKar to use your location to find nearby mechanics"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/notification-icon.png",
          "color": "#00ACC1"
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1b5c2494-b755-40d9-9a8d-4711c76c212e"
      }
    },
    "runtimeVersion": "1.0.0",
    "owner": "ahmad634"
  }
}
