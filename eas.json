{
    "cli": {
        "version": ">= 5.2.0"
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal"
        },
        "preview": {
            "distribution": "internal",
            "developmentClient": false,
            "channel": "preview",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_OTP_DEV_MODE": "false",
                "EXPO_NO_DEV_CLIENT": "true",
                "EAS_BUILD_ANDROID_ABI": "arm64-v8a"
            }
        },
        "production": {
            "developmentClient": false,
            "channel": "production",
            "android": {
                "buildType": "apk"
            },
            "env": {
                "EXPO_PUBLIC_OTP_DEV_MODE": "false",
                "EXPO_NO_DEV_CLIENT": "true"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
